{
    "name": "GitBreakdown",
    "version": "0.7",
    "description": "GitBreakdown Chrome extension",
    "permissions": 
    [
        "declarativeContent",
        "storage",
        "http://18.215.242.203:3000/*",
        "tabs"
    ],
    "background": 
    {
        "scripts": ["scripts/background.js"],
        "persistent": false
    },
    "content_scripts": 
    [{
        "matches": 
        [
            "http://github.com/*",
            "https://github.com/*",
            "http://*.github.com/*",
            "https://*.github.com/*",
            "http://18.215.242.203:3000/*"
        ],
        "js": 
        [
            "scripts/contentScripts.js",
            "scripts/oauth.js",
            "scripts/Chart.min.js",
            "scripts/graph.js",
            "scripts/timeConfigPage.js",
            "scripts/branchesPage.js",            
            "scripts/commitsPage.js",
            "scripts/issuesPage.js",
            "scripts/prPage.js",
            "scripts/screenPage.js",
            "scripts/observers.js",
            "scripts/zenhubRequirements.js",
            "scripts/button.js",
            "scripts/screenConfig.js"
            

        ],
        "css": 
        [
            "styles/gbdStyle.css"
        ]
    }],
    "browser_action": 
    {
        "default_popup": "popup.html"
    },
    "manifest_version": 2
  }
