{
    "name": "GitBreakdown",
    "version": "0.7",
    "description": "GitBreakdown Chrome extension",
    "web_accessible_resources": ["images/logo.jpg", "images/cog-8x.png"],
    "permissions": 
    [
        "declarativeContent",
        "storage",
        "http://18.215.242.203:3000/*",
        "tabs"
    ],
    "background": 
    {
        "scripts": ["scripts/background.js"],
        "persistent": false
    },
    "content_scripts": 
    [{
        "matches": 
        [
            "http://github.com/*",
            "https://github.com/*",
            "http://*.github.com/*",
            "https://*.github.com/*",
            "http://18.215.242.203:3000/*"
        ],
        "js": 
        [
            "scripts/oauth.js",
            "scripts/jquery.min.js",
            "scripts/popper.min.js",
            "scripts/bootstrap.min.js",
            "scripts/Chart.min.js",
            "scripts/settings.js",
            "scripts/graph.js",
            "scripts/branchesPage.js",            
            "scripts/commitsPage.js",
            "scripts/issuesPage.js",
            "scripts/prPage.js",
            "scripts/screenPage.js",
            "scripts/observers.js",
            "scripts/zenhubRequirements.js",
            "scripts/button.js",
            "scripts/screenConfig.js",
            "scripts/popup.js"
        ],
        "css": 
        [
            "styles/bootstrap.min.css",
            "styles/gbdStyle.css",
            "styles/githubWebKit.css",
            "styles/githubMainPageWebKit.css",
            "styles/popupStyle.css"

        ]
    }],
    "browser_action": 
    {
        "default_popup": "popup/popup.html"
    },
    "manifest_version": 2
  }
