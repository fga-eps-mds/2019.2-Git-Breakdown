{
    "name": "GitBreakdown",
    "version": "0.7",
    "description": "GitBreakdown Chrome extension",
    "permissions": 
    [
        "declarativeContent",
        "storage",
        "http://18.215.242.203:3000/*",
        "tabs"
    ],
    "background": 
    {
        "scripts": ["scripts/background.js"],
        "persistent": false
    },
    "content_scripts": 
    [{
        "matches": 
        [
            "http://github.com/*",
            "https://github.com/*",
            "http://*.github.com/*",
            "https://*.github.com/*",
            "http://18.215.242.203:3000/*"
        ],
        "js": 
        [
            "scripts/oauth.js",
            "scripts/contentScripts.js",
            "scripts/button.js",
            "scripts/screen.js",
            "scripts/Chart.min.js"
            
        ]
    }],
    "browser_action": 
    {
        "default_popup": "popup.html"
    },
    "manifest_version": 2
  }
